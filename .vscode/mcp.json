{
    "servers": {
        "chsarp-mcp-stdio": {
            "type": "stdio",
            "command": "dotnet",
            "args": [
                "run",
                "--project",
                "${workspaceFolder}/src/csharp/Stdio/MCP.Stdio.Server/MCP.Stdio.Server.csproj"
            ]
        },
        "node-MCP-stdio": {
            "type": "stdio",
            "command": "node",
            "args": [
                "${workspaceFolder}/src/ts/stdio/server/dist/app.js"
            ]
        },
        "node-MCP-http": {
            "url": "http://localhost:3000/mcp"
        }
    }
}